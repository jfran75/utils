[
  {
    "key": "ctrl+alt+=",
    "command": "workbench.action.terminal.runSelectedText"
  },
  {
    "key": "ctrl+shift+1",
    "command": "workbench.action.terminal.focusAtIndex1"
  },
  {
    "key": "ctrl+shift+2",
    "command": "workbench.action.terminal.focusAtIndex2"
  },
  {
    "key": "ctrl+shift+3",
    "command": "workbench.action.terminal.focusAtIndex3"
  },
  {
    "key": "ctrl+shift+4",
    "command": "workbench.action.terminal.focusAtIndex4"
  },
  {
    "key": "ctrl+shift+5",
    "command": "workbench.action.terminal.focusAtIndex5"
  },
  {
    "key": "ctrl+shift+5",
    "command": "workbench.action.terminal.focusAtIndex5"
  },
  {
    "key": "ctrl+shift+alt+right",
    "command": "workbench.action.terminal.focusNext"
  },
  {
    "key": "ctrl+shift+alt+left",
    "command": "workbench.action.terminal.focusPrevious"
  },
  {
    "key": "r",
    "command": "renameFile",
    "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "j",
    "command": "list.focusDown",
    "when": "listFocus && !inputFocus"
  },
  {
    "key": "k",
    "command": "list.focusUp",
    "when": "listFocus && !inputFocus"
  },

  {
    "key": "enter",
    "command": "list.select",
    "when": "explorerViewletVisible && filesExplorerFocus"
  },

  {
    "key": "a",
    "command": "explorer.newFile",
    "when": "filesExplorerFocus && !inputFocus"
  },
  {
    "key": "shift+a",
    "command": "explorer.newFolder",
    "when": "filesExplorerFocus && !inputFocus"
  },
  {
    "key": "ctrl+shift+t",
    "command": "workbench.action.toggleMaximizedPanel"
  },
  {
    "key": "ctrl+shift+[",
    "command": "workbench.action.focusOtherSideEditor"
  },
  {
    "key": "shift+delete",
    "command": "editor.action.deleteLines",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "key": "shift+cmd+k",
    "command": "-editor.action.deleteLines",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "key": "alt+cmd+d",
    "command": "workbench.action.decreaseViewWidth"
  },
  {
    "key": "alt+cmd+a",
    "command": "workbench.action.increaseViewWidth"
  },
  {
    "key": "alt+cmd+w",
    "command": "workbench.action.decreaseViewHeight"
  },
  {
    "key": "alt+cmd+s",
    "command": "workbench.action.increaseViewHeight"
  },
  {
    "key": "ctrl+shift+x",
    "command": "workbench.action.terminal.kill"
  },
  {
    "key": "ctrl+shift+,",
    "command": "moveActiveEditor",
    "args": {
      "to": "left",
      "by": "tab",
      "value": 1
    }
  },
  {
    "key": "ctrl+shift+.",
    "command": "moveActiveEditor",
    "args": {
      "to": "right",
      "by": "tab",
      "value": 1
    }
  },
  {
    "key": "shift+cmd+r", // open recent
    "command": "workbench.action.openRecent"
  },
  {
    "key": "shift+cmd+r", // remove keybinding
    "command": "-rerunSearchEditorSearch",
    "when": "inSearchEditor"
  },
  {
    "key": "cmd+\\",
    "command": "-workbench.action.terminal.split",
    "when": "terminalFocus && terminalProcessSupported || terminalFocus && terminalWebExtensionContributedProfile"
  },
  {
    "key": "alt+cmd+-",
    "command": "workbench.action.splitEditorDown"
  },
  {
    "key": "ctrl+alt+o",
    "command": "openInIntegratedTerminal"
  },
  {
    "key": "alt+cmd+j",
    "command": "-liveshare.join",
    "when": "liveshare:state != 'Joined' && liveshare:state != 'Shared'"
  }
]